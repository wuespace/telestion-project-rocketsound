{
	"name": "telestion-project-rocketsound",
	"description": "telestion-project-rocketsound, a Telestion PSC generated using the telestion-client-cli",
	"version": "1.0.0",
	"homepage": ".",
	"engines": {
		"node": ">=14",
		"npm": ">=7"
	},
	"scripts": {
		"prepare": "npm run meta:register-precommit",
		"start": "npm run start:electron",
		"start:electron": "tc-cli start --electron",
		"start:browser": "tc-cli start --browser",
		"build": "tc-cli build",
		"stats": "tc-cli stats",
		"style": "prettier --write package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"style:ci": "prettier --list-different package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/ --no-error-on-unmatched-pattern src",
		"ts": "tsc",
		"clean": "rimraf build dist",
		"meta:register-precommit": "node scripts/register-precommit-hook.js"
	},
	"private": true,
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@adobe/react-spectrum": "^3.12.0",
		"@fliegwerk/logsemts": "^0.4.2",
		"@monsonjeremy/react-leaflet": "^3.2.2",
		"@monsonjeremy/react-leaflet-core": "^1.1.1",
		"@react-leaflet/core": "npm:@monsonjeremy/react-leaflet-core@^1.1.1",
		"@react-spectrum/table": "3.0.0-beta.1",
		"@spectrum-icons/illustrations": "^3.2.1",
		"@spectrum-icons/ui": "^3.2.0",
		"@spectrum-icons/workflow": "^3.2.0",
		"@wuespace/telestion-client-common": "^0.17.0",
		"@wuespace/telestion-client-core": "^0.17.0",
		"@wuespace/telestion-client-prop-types": "^0.17.0",
		"@wuespace/vertx-mock-server": "^0.17.0",
		"chalk": "^4.1.2",
		"electron": "^13.1.7",
		"leaflet": "^1.7.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-leaflet": "npm:@monsonjeremy/react-leaflet@^3.2.2",
		"react-router-dom": "^5.2.0",
		"react-virtualized-auto-sizer": "^1.0.5",
		"recharts": "^2.0.10",
		"zustand": "^3.5.7"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.2.1",
		"@types/jest": "^26.0.24",
		"@types/leaflet": "^1.7.4",
		"@types/node": "^16.4.7",
		"@types/react": "^17.0.15",
		"@types/react-dom": "^17.0.9",
		"@types/react-virtualized-auto-sizer": "^1.0.1",
		"@types/recharts": "^2.0.0",
		"@wuespace/telestion-client-cli": "^0.17.0",
		"@wuespace/telestion-client-types": "^0.17.0",
		"prettier": "^2.3.2",
		"pretty-quick": "^3.1.1",
		"react-scripts": "^4.0.3",
		"rimraf": "^3.0.2",
		"typescript": "^4.3.5"
	}
}
